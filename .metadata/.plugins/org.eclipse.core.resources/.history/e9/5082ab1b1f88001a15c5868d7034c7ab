	public static void main(String[] args) {
		try {
			//클라이언트와 통신할 수 있도록 준비
			ServerSocket server = new ServerSocket(12345);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
